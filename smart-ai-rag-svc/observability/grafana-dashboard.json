{
  "dashboard": {
    "title": "Smart AI RAG Service - Dashboard",
    "tags": ["rag", "ai", "openai", "llamaindex", "langchain"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Request Rate",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Request Latency (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "stat",
        "title": "Active Requests",
        "targets": [
          {
            "expr": "active_requests",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "stat",
        "title": "Total Documents Processed",
        "targets": [
          {
            "expr": "sum(documents_processed_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "stat",
        "title": "Total Questions Answered",
        "targets": [
          {
            "expr": "sum(questions_answered_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Document Processing Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(document_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{framework}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(document_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{framework}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Question Answering Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(question_answering_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{framework}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(question_answering_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{framework}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"}
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "type": "graph",
        "title": "LLM Token Usage",
        "targets": [
          {
            "expr": "rate(llm_tokens_used_total[5m])",
            "legendFormat": "{{model}} - {{type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Tokens/sec"}
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "type": "graph",
        "title": "OpenAI Cost (Cumulative)",
        "targets": [
          {
            "expr": "openai_cost_dollars_total",
            "legendFormat": "{{model}} - {{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "Cost"}
        ]
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "type": "graph",
        "title": "Error Rate",
        "targets": [
          {
            "expr": "rate(errors_total[5m])",
            "legendFormat": "{{type}} - {{component}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "errps", "label": "Errors/sec"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.1], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "type": "graph",
        "title": "Evaluation Scores",
        "targets": [
          {
            "expr": "evaluation_scores",
            "legendFormat": "{{metric}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Score", "max": 1, "min": 0}
        ]
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "type": "graph",
        "title": "Vector Store Operations",
        "targets": [
          {
            "expr": "rate(vector_store_operations_total[5m])",
            "legendFormat": "{{operation}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"}
        ]
      },
      {
        "id": 13,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "type": "graph",
        "title": "LLM Request Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(llm_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{provider}} {{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"}
        ]
      },
      {
        "id": 14,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 48},
        "type": "table",
        "title": "HTTP Status Codes",
        "targets": [
          {
            "expr": "sum by (status, endpoint) (http_requests_total)",
            "format": "table",
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "status": "Status Code",
                "endpoint": "Endpoint",
                "Value": "Count"
              }
            }
          }
        ]
      }
    ]
  }
}

